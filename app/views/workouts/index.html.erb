<h1>Listing Workouts</h1>
 <%= link_to 'Create a new workout', new_workout_path, {:style=> 'size:150px'} %>
<table>
  <tr>
    <th>Workouts</th>
    <th>Description</th>
    <th>Date</th>
  </tr>
 
  <% @workouts.each do |workout| %>
    <tr>
      <td><%= workout.workout %></td>
      <td><%= workout.description %></td>
      <td><%= workout.date %></td>
      <td><%= link_to 'Show', workout_path(workout) %></td>
    </tr>
  <% end %>
</table>